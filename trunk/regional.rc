#BCC
VERSION=BCB.01
!ifndef CC_DIR
CC_DIR = $(MAKE_DIR)
!endif

!ifndef HB_DIR
HB_DIR = $(HARBOUR_DIR)
!endif
 
RECURSE= NO 
 
COMPRESS = NO
EXTERNALLIB = YES
XFWH = NO
FILESTOADD =  5
WARNINGLEVEL =  0
USERDEFINE = 
USERINCLUDE = 
GUI = NO
MT = NO
SRC03 = obj 
PROJECT = reg32.exe $(PR) 
OBJFILES = $(SRC03)\REGIONAL.obj $(SRC03)\AFILACT.obj $(SRC03)\ALERTIT.obj $(SRC03)\ARCHIVOS.obj $(SRC03)\AUDFARM.obj //
 $(SRC03)\AUDIT.obj $(SRC03)\AUDPRAC.obj $(SRC03)\BANCO.obj $(SRC03)\BORRAORD.obj $(SRC03)\CARGAUT.obj //
 $(SRC03)\COMPRO.obj $(SRC03)\CONFCHEQ.obj $(SRC03)\CONV.obj $(SRC03)\ENVIAR.obj $(SRC03)\FAX.obj //
 $(SRC03)\FREC.obj $(SRC03)\IMPAUD.obj $(SRC03)\IMPCHEQU.obj $(SRC03)\IMPFACT.obj $(SRC03)\IMPFAX.obj //
 $(SRC03)\IMPMESA.obj $(SRC03)\IMPREINT.obj $(SRC03)\INF01.obj $(SRC03)\INGAUD.obj $(SRC03)\I_MOUSE.obj //
 $(SRC03)\LETRAS.obj $(SRC03)\LISDEB.obj $(SRC03)\LISTAC11.obj $(SRC03)\LISTREIN.obj $(SRC03)\MESA.obj //
 $(SRC03)\ORDAFIL.obj $(SRC03)\PEDIDO.obj $(SRC03)\REINFAR.obj $(SRC03)\REINPRAC.obj $(SRC03)\RENDIC.obj //
 $(SRC03)\RENDTODO.obj $(SRC03)\SET_ELI.obj $(SRC03)\SUPMENU.obj $(SRC03)\TRAER.obj $(SRC03)\UDF.obj //
 $(SRC03)\VERAPE.obj $(SRC03)\VERORDAF.obj $(OB) 
PRGFILES = REGIONAL.PRG AFILACT.PRG ALERTIT.PRG ARCHIVOS.PRG AUDFARM.PRG //
 AUDIT.PRG AUDPRAC.PRG BANCO.PRG BORRAORD.PRG CARGAUT.PRG //
 COMPRO.PRG CONFCHEQ.PRG CONV.PRG ENVIAR.PRG FAX.PRG //
 FREC.PRG IMPAUD.PRG IMPCHEQU.PRG IMPFACT.PRG IMPFAX.PRG //
 IMPMESA.PRG IMPREINT.PRG INF01.PRG INGAUD.PRG I_MOUSE.PRG //
 LETRAS.PRG LISDEB.PRG LISTAC11.PRG LISTREIN.PRG MESA.PRG //
 ORDAFIL.PRG PEDIDO.PRG REINFAR.PRG REINPRAC.PRG RENDIC.PRG //
 RENDTODO.PRG SET_ELI.PRG SUPMENU.PRG TRAER.PRG UDF.PRG //
 VERAPE.PRG VERORDAF.PRG $(PS) 
OBJCFILES = $(OBC) 
CFILES = $(CF)
RESFILES = 
RESDEPEN = 
TOPMODULE = REGIONAL.PRG
LIBFILES = optcon.lib  lang.lib vm.lib rtl.lib rdd.lib macro.lib pp.lib dbfntx.lib dbfcdx.lib dbffpt.lib common.lib gtwin.lib codepage.lib ct.lib tip.lib pcrepos.lib hsx.lib hbsix.lib  super.lib
EXTLIBFILES = super.lib
DEFFILE = 
HARBOURFLAGS =  -m  -w0
CFLAG1 =  -OS $(CFLAGS) -d -c -L$(HB_DIR)\lib 
CFLAG2 =  -I$(HB_DIR)\include;$(CC_DIR)\include
RFLAGS = 
LFLAGS = -L$(CC_DIR)\lib\obj;$(CC_DIR)\lib;$(HB_DIR)\lib -Gn -M -m -s -Tpe -x -ap
IFLAGS = 
LINKER = ilink32
 
ALLOBJ = c0x32.obj $(OBJFILES) $(OBJCFILES)
ALLRES = $(RESDEPEN)
ALLLIB = $(LIBFILES) import32.lib cw32.lib
.autodepend
 
#DEPENDS
 
#COMMANDS
.cpp.obj:
$(CC_DIR)\BIN\bcc32 $(CFLAG1) $(CFLAG2) -o$* $**
 
.c.obj:
$(CC_DIR)\BIN\bcc32 -I$(HB_DIR)\include $(CFLAG1) $(CFLAG2) -o$* $**
 
.prg.obj:
$(HB_DIR)\bin\harbour -D__EXPORT__ -n -go -I$(HB_DIR)\include $(HARBOURFLAGS) -o$* $**
 
.rc.res:
$(CC_DIR)\BIN\brcc32 $(RFLAGS) $<
 
#BUILD
 
$(PROJECT): $(CFILES) $(OBJFILES) $(RESDEPEN) $(DEFFILE)
    $(CC_DIR)\BIN\$(LINKER) @&&!  
    $(LFLAGS) +
    $(ALLOBJ), +
    $(PROJECT),, +
    $(ALLLIB), +
    $(DEFFILE), +
    $(ALLRES) 
!
